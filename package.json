{
  "name": "webpack-mem-compile",
  "version": "1.0.2",
  "description": "Webpack compile to memory",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "babel": "babel ./src --ignore=node_modules --out-dir ./dist --source-maps inline --extensions .ts,.tsx",
    "build": "npm run clean && npm run babel && npm run build:types",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:flow": "find . -type f -not -path './node_modules/*' -name '*.d.ts' -exec sh -c 'flowgen --add-flow-header $1 -o ${1%.*.*}.js.flow' _ '{}' \\;",
    "build:types": "npm run build:tsc && npm run build:flow",
    "clean": "rimraf dist",
    "lint": "eslint --ext js,jsx,ts,tsx src/ test/",
    "jest": "jest test --no-cache --coverage --collectCoverageFrom=./src --verbose --runInBand --silent=false",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run jest",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:krakenjs/webpack-mem-compile.git"
  },
  "author": "dbrain",
  "license": "MIT",
  "engines": {
    "node": ">= 6"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/memory-fs": "^0.3.3",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-core": "^7.0.0-bridge.0",
    "flowgen": "^1.14.1",
    "grumbler-scripts": "^3",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "uuid": "^3.3.2"
  }
}
